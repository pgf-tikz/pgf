  \pgfutil@ifnextchar\relax{%
    \edef\pgf@sys@svg@parsed@dash{\pgf@sys@svg@parsed@dash\pgf@sys@tonumber\pgf@x}%
  }{%
    \edef\pgf@sys@svg@parsed@dash{\pgf@sys@svg@parsed@dash\pgf@sys@tonumber\pgf@x,}%
    \pgf@sys@svg@parse@dash%
  }%
}
