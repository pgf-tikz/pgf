% Path usage:
\newif\ifpgf@sys@svg@clipnext
\def\pgf@sys@svg@possiblyclippedpath#1{%
  \ifpgf@sys@svg@clipnext%
    \global\advance\pgf@sys@svg@objectcount by1\relax%
    \pgfsysprotocol@literal{<clipPath id="\pgf@sys@svg@if@fresh@id{\pgf@sys@svg@id@key}{pgfcp\the\pgf@sys@svg@objectcount}clip"> <path id="\pgf@sys@svg@if@fresh@id{\pgf@sys@svg@id@key}{pgfcp\the\pgf@sys@svg@objectcount}" d="}%
    \pgf@sys@flushsvgpath%
    \pgfsysprotocol@literal{"/> </clipPath>\pgfsys@svg@newline }
    \pgfsysprotocol@literal{<use xlink:href="\#\pgf@sys@svg@if@fresh@id{\pgf@sys@svg@id@key}{pgfcp\the\pgf@sys@svg@objectcount}" #1/>\pgfsys@svg@newline }%
    \pgf@sys@svg@gs{clip-path="url(\#\pgf@sys@svg@if@fresh@id{\pgf@sys@svg@id@key}{pgfcp\the\pgf@sys@svg@objectcount}clip)"}
    \pgf@sys@svg@clipnextfalse%
  \else%
    \pgfsysprotocol@literal{<path\pgf@sys@svg@if@fresh@id{ id="\pgf@sys@svg@id@key"}{} d="}%
    \pgf@sys@flushsvgpath%
    \pgfsysprotocol@literal{" #1/>\noexpand\pgfsys@svg@newline }
  \fi%
